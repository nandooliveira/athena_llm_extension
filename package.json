{
  "name": "atena",
  "displayName": "Atena",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "atena.startNewSession",
        "title": "Start a new Session"
      }
    ],
    "configuration": {
      "title": "Atena",
      "properties": {
        "atena.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI Api Key"
        },
        "atena.openAIModel": {
          "type": "string",
          "default": "gpt-4",
          "enum": ["gpt-4", "gpt-3.5-turbo", "davinci-002"],
          "description": "OpenAI Api Model"
        },
        "atena.enableTelemetry": {
          "type": "boolean",
          "default": false,
          "description": "Enable/disable sending telemetry data."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "openai": "^4.11.0",
    "uuid": "^9.0.1"
  }
}
